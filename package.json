{
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port=8080",
    "generate": "NODE_OPTIONS=--max-old-space-size=7168 nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "lint-fix": "eslint --fix --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "fast-deploy": "npm run generate && aws s3 sync ./.output/public s3://mainsitestack-development-site-content && aws s3 sync ./.output/public s3://mainsitestack-development-site-content",
    "deploy-only": "aws s3 sync ./.output/public s3://mainsitestack-development-site-content",
    "serve": "npm run generate && npx serve -p 8080 .output/public",
    "serve-only": "npx serve -p 8080 .output/public",
    "validate": "poetry run html5validator --root ./.output/public --blacklist storybook --ignore 'CSS: \"display\": Parse Error' 'Bad value  for attribute \"src\" on element \"img\": Illegal character in scheme data: space is not allowed.' 'The element \"button\" must not appear as a descendant of the \"a\" element.' 'Duplicate attribute'",
    "link_check": "poetry run python crawler.py",
    "dist_verify": "node builtDistVerify",
    "algolia_populate": "node algoliaPopulate",
    "test": "vitest run --coverage",
    "e2e": "npx playwright test",
    "e2e:update-snapshots": "npx playwright test --update-snapshots",
    "e2e:report": "npx playwright test --reporter dot",
    "e2e:web": "npx playwright show-report",
    "storybook": "storybook dev --port 6006",
    "build-storybook": "storybook build"
  },
  "playwright": {
    "testDir": "tests"
  },
  "overrides": {
    "vue": "latest"
  },
